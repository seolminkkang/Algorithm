package inflean.array;

import java.util.Scanner;

public class Array03 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int[] aArr = new int[n];
		int[] bArr = new int[n];
		
		for(int i =0; i<n;i++) {
			aArr[i] = sc.nextInt();
			bArr[i] = sc.nextInt();
		}
		
	}

}
